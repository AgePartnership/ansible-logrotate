# {{ ansible_managed }}

{% if item.path is defined -%}"{{ item.path }}"{%- endif %}
{% for path in item.paths|default([]) -%}
{{path}}
{%- endfor %}
{
  {% if item.options is defined -%}
  {% for option in item.options -%}
  {{ option }}
  {% endfor -%}
  {% endif %}
  {%- if item.scripts is defined -%}
  {%- for name, script in item.scripts.items() -%}
  {{ name }}
    {{ script }}
  endscript
  {% endfor -%}
  {% endif -%}
}
